{
  "name": "backend-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:dev": "NODE_ENV=development nest build",
    "build:staging": "NODE_ENV=staging nest build",
    "build:prod": "NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:staging": "NODE_ENV=staging nest start",
    "start:prod": "NODE_ENV=production node dist/main",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "commit": "cz",
    "prepare": "husky",
    "prisma:generate": "npx dotenv -e .env -- npx prisma generate",
    "prisma:generate:dev": "npx dotenv -e .env.development -- npx prisma generate",
    "prisma:generate:staging": "npx dotenv -e .env.staging -- npx prisma generate",
    "prisma:generate:prod": "npx dotenv -e .env.production -- npx prisma generate",
    "prisma:push": "npx dotenv -e .env -- npx prisma db push",
    "prisma:push:dev": "npx dotenv -e .env.development -- npx prisma db push",
    "prisma:push:staging": "npx dotenv -e .env.staging -- npx prisma db push",
    "prisma:push:prod": "npx dotenv -e .env.production -- npx prisma db push",
    "prisma:migrate": "npx dotenv -e .env -- npx prisma migrate dev",
    "prisma:migrate:dev": "npx dotenv -e .env.development -- npx prisma migrate dev",
    "prisma:migrate:staging": "npx dotenv -e .env.staging -- npx prisma migrate deploy",
    "prisma:migrate:prod": "npx dotenv -e .env.production -- npx prisma migrate deploy",
    "prisma:studio": "npx dotenv -e .env -- npx prisma studio",
    "prisma:studio:dev": "npx dotenv -e .env.development -- npx prisma studio",
    "prisma:studio:staging": "npx dotenv -e .env.staging -- npx prisma studio",
    "prisma:studio:prod": "npx dotenv -e .env.production -- npx prisma studio"
  },
  "dependencies": {
    "@nestjs/class-transformer": "^0.4.0",
    "@nestjs/class-validator": "^0.13.4",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.15.0",
    "@vercel/node": "^5.3.21",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "dotenv": "^17.2.2",
    "helmet": "^8.1.0",
    "prisma": "^6.15.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
