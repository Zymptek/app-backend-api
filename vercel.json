{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/index.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "SWAGGER_ENABLED": "true",
    "VERCEL": "1",
    "API_PREFIX": "api/v1",
    "CORS_ORIGIN": "*"
  }
}
