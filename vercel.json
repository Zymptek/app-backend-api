{
  "version": 2,
  "buildCommand": "npm run vercel:build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "nodejs",
  "functions": {
    "dist/main.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "SWAGGER_ENABLED": "true",
    "VERCEL": "1"
  }
}
